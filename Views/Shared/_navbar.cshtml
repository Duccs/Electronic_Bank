@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>@ViewData["Title"] - Electronic Bank</title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="~/vendors/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="~/vendors/base/vendor.bundle.base.css">
  <!-- endinject -->
  <!-- plugin css for this page -->
  <link rel="stylesheet" href="~/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
  <!-- End plugin css for this page -->
  <!-- inject:css -->
  <link rel="stylesheet" href="~/css/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="~/images/favicon.png" />
  <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
</head>
<body>
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="navbar-brand-wrapper d-flex justify-content-center">
                <div class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100">
                    <a class="navbar-brand brand-logo" asp-area="" asp-controller="Home" asp-action="Index">Electronic Bank</a>
                    <a class="navbar-brand brand-logo-mini" asp-area="" asp-controller="Home" asp-action="Index">Electronic Bank</a>
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-sort-variant"></span>
                    </button>
                </div>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                            <i class="mdi mdi-home menu-icon"></i>
                            <span class="menu-title">Dashboard</span>
                        </a>
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Clients" asp-action="Index">
                            <i class="mdi mdi-account-multiple menu-icon"></i>
                            <span class="menu-title">Clients</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Currencies" asp-action="Index">
                            <i class="mdi mdi-currency-usd  menu-icon"></i>
                            <span class="menu-title">Currencies</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Wallets" asp-action="Index">
                            <i class="mdi mdi-wallet  menu-icon"></i>
                            <span class="menu-title">Wallets</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Transactions" asp-action="Index">
                            <i class="mdi mdi-cash-multiple  menu-icon"></i>
                            <span class="menu-title">Transactions History</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#auth" aria-expanded="false" aria-controls="auth">
                            <i class="mdi mdi-newspaper  menu-icon"></i>
                            <span class="menu-title">Bank Pages</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="auth">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-area="" asp-controller="Clients" asp-action="Index"> Clients </a></li>
                                <li class="nav-item"> <a class="nav-link" asp-area="" asp-controller="Wallets" asp-action="Index"> Wallets </a></li>
                                <li class="nav-item"> <a class="nav-link" asp-area="" asp-controller="Transactions" asp-action="Index"> Transactions </a></li>
                                <li class="nav-item"> <a class="nav-link" asp-area="" asp-controller="Currencies" asp-action="Index"> Currencies </a></li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="container">
                        <main role="main" class="pb-3">
                            @RenderBody()
                        </main>
                    </div>
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © <a href="https://github.com/Duccs" target="_blank">Yosof </a>2022</span>
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Only the best <a href="https://github.com/Duccs" target="_blank"> ASP Systems  </a></span>
                    </div>
                </footer>
            <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/vendors/chart.js/Chart.min.js"></script>
    <script src="~/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/js/off-canvas.js"></script>
    <script src="~/js/hoverable-collapse.js"></script>
    <script src="~/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/js/dashboard.js"></script>
    <script src="~/js/data-table.js"></script>
    <script src="~/js/jquery.dataTables.js"></script>
    <script src="~/js/dataTables.bootstrap4.js"></script>
    <script src="~/js/dataTables.bootstrap4.js"></script>
    <!-- End custom js for this page-->

    <script src="~/js/jquery.cookie.js" type="text/javascript"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
